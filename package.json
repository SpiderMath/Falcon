{
  "name": "falcon",
  "version": "1.0.0",
  "description": "### Functionalities (Personal)",
  "main": "index.js",
  "scripts": {
    "test": "ts-node-dev --respawn --transpile-only --poll ./src/index.ts",
    "lint": "prettier --write .",
    "build": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-angular": "^13.1.0",
    "@types/express": "^4.17.13",
    "@types/node": "^16.4.13",
    "@types/os-utils": "^0.0.1",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "node": "^16.6.0",
    "prettier": "^2.3.2",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn dlx @yarnpkg/doctor && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": "prettier --write",
    "*.js": "prettier --write"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ],
    "rules": {
      "scope-case": [
        2,
        "always",
        "pascal-case"
      ],
      "type-enum": [
        2,
        "always",
        [
          "chore",
          "build",
          "ci",
          "docs",
          "feat",
          "fix",
          "perf",
          "refactor",
          "revert",
          "style",
          "test"
        ]
      ]
    }
  },
  "dependencies": {
    "@discordjs/opus": "^0.5.3",
    "@vitalets/google-translate-api": "^7.0.0",
    "@weky/inlinereply": "^0.0.0",
    "aki-api.ts": "^1.0.1",
    "axios": "^0.21.1",
    "common-tags": "^1.8.0",
    "consola": "^2.15.3",
    "deta": "^1.0.0",
    "discord-buttons": "^4.0.0",
    "discord-giveaways": "^4.5.1",
    "discord-multipurpose": "^2.1.14",
    "discord-nsfw": "^1.3.0",
    "discord-tictactoe": "^2.1.0",
    "discord.js": "^12.5.3",
    "discord.js-collector": "^1.8.9",
    "distube": "^2.8.15",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "ffmpeg-static": "^4.4.0",
    "findthelyrics": "^1.1.0",
    "google-it": "^1.6.2",
    "mathjs": "^9.4.2",
    "moment": "^2.29.1",
    "moment-duration-format": "^2.3.2",
    "ms": "^2.1.3",
    "mysql2": "^2.2.5",
    "nuggies": "^1.2.6",
    "opusscript": "0.0.8",
    "os-utils": "^0.0.14",
    "reconlx": "^1.3.2",
    "sequelize": "^6.6.2",
    "snakecord": "^1.0.7",
    "txtgen": "^2.2.8",
    "weather-js": "^2.0.0",
    "weky": "^3.1.0",
    "wikijs": "^6.2.3"
  }
}